{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "cleanup": "tsc --build ./ --clean && cdk context --clear && rm -rf dist cdk.out && rm -f package-lock.json",
    "build": "tsc",
    "watch": "tsc -w",
    "pretest": "sh pretest.sh",
    "test": "jest --coverage",
    "cdk": "cdk",
    "presynth": "sh pretest.sh && npm run build",
    "synth": "cdk synth -q"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.6",
    "aws-cdk": "2.67.0",
    "cdk-nag": "^2.22.24",
    "jest": "^29.4.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.67.0",
    "constructs": "^10.0.0"
  }
}
